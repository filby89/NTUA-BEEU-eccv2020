{
    "name": "pycharm debug",
    "n_gpu": 4,

    "loss": "combined_loss",

    "loss_continuous": "mse_loss",
    "metrics": [
        "average_precision", "roc_auc"
    ],

    "metrics_continuous": [
        "r2", "mean_squared_error"
    ],

    "lr_scheduler": {
        "type": "MultiStepLR",
        "args": {
            "milestones": [10, 20],
            "gamma": 0.1
        }
    },

    "trainer": {
        "epochs": 20,

        "save_dir": "log",
        "save_period": 5,
        "verbosity": 2,
        
        "monitor": "min val_loss",
        "early_stop": 100,

        "tensorboard": true
    }
}
